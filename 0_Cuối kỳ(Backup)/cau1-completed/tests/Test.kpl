Program Fibo;
var n : integer;
    sum : integer;
    
Function ZZZ(n : integer) : Integer;
Begin
End;

Function Fibonaci(n : integer) : Integer;
Begin
    if n <= 0 then Fibonaci := 0;
    if n = 1 then Fibonaci := 1;
    if n > 1 then Fibonaci := Fibonaci(n - 1) + Fibonaci(n - 2) + ZZZ(n);
End;



Begin
   n := readi;
   sum := Fibonaci(n);
   call writei(sum);
End.
